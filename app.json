{
  "expo": {
    "name": "DakLak Coffee",
    "slug": "daklak-coffee",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.jpg",
    "scheme": "daklakcoffee",
    "description": "Ứng dụng quản lý chuỗi cung ứng cà phê DakLak",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.daklakcoffee.app",
      "buildNumber": "1",
      "infoPlist": {
        "CFBundleDisplayName": "DakLak Coffee",
        "CFBundleName": "DakLak Coffee"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.daklakcoffee.app",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6d55a203-343a-418a-aa33-04e23f940232"
      }
    },
    "owner": "minh25031",
    "splash": {
      "image": "./assets/images/logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    }
  }
}
